DART KOD
--
import 'dart:io';
void main() {
  print("SAYI GİR!");
  double sonuc = 0.0;
  int n = int.parse(stdin.readLineSync()!);
  for(int i = 1; i <= n; i++)
    {
      sonuc = i+(i/i+1);
    }
  print(sonuc as double);

}
