import 'package:flutter/material.dart';

void main() {
  runApp(MyApp());
}

class MyApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      home: Scaffold(
        backgroundColor: Colors.grey,
        appBar: AppBar(
          centerTitle: true,
          title: Text('Et Lokantası'),
          backgroundColor: Colors.black38,
        ),
        body: Column(
          mainAxisAlignment: MainAxisAlignment.center,
          children: [
            Text(
              'Aşağıdaki menülerden istediğinizi seçiniz!',
              style: TextStyle(
                fontSize: 16,
                fontWeight: FontWeight.bold,
              ),
            ),
            SizedBox(height: 20),
            Row(
              mainAxisAlignment: MainAxisAlignment.center,
              children: [
                CircleAvatar(
                  radius: 70,
                  backgroundImage: AssetImage('images/resim1.jpg'),
                ),
                SizedBox(width: 25),
                CircleAvatar(
                  radius: 70,
                  backgroundImage: AssetImage('images/resim2.jpg'),
                ),
              ],
            ),
            SizedBox(height: 20),
            Row(
              mainAxisAlignment: MainAxisAlignment.center,
              children: [
                CircleAvatar(
                  radius: 70,
                  backgroundImage: AssetImage('images/resim3.jpg'),
                ),
                SizedBox(width: 25),
                CircleAvatar(
                  radius: 70,
                  backgroundImage: AssetImage('images/resim4.jpg'),
                ),
              ],
            ),
          ],
        ),
      ),
    );
  }
}
